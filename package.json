{
    "name": "diznary-monorepo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "test": "turbo run test"
    },
    "devDependencies": {
        "turbo": "latest",
        "prettier": "^3.4.2"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "npm@10.0.0"
}