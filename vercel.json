{
    "version": 2,
    "builds": [
        {
            "src": "apps/backend/src/vercel.ts",
            "use": "@vercel/node"
        },
        {
            "src": "apps/frontend/package.json",
            "use": "@vercel/next"
        }
    ],
    "rewrites": [
        {
            "source": "/api/v1/(.*)",
            "destination": "/apps/backend/src/vercel.ts"
        }
    ]
}